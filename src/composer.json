{
  "name": "slayerfat/sistemaPCI",
  "description": "Sistema de gestion de inventario, Ingeniera en informatica, IUTOMS.",
  "keywords": [
    "gestion",
    "inventario",
    "ingenieria",
    "informatica"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.1.*",
    "barryvdh/laravel-ide-helper": "^2.1",
    "doctrine/dbal": "^2.5",
    "artesaos/seotools": "v0.7.3.5",
    "jenssegers/date": "^3.0",
    "laracasts/flash": "^1.3",
    "laravelcollective/html": "^5.1",
    "patricktalmadge/bootstrapper": "~5",
    "cviebrock/eloquent-sluggable": "^3.0",
    "icanboogie/inflector": "^1.3",
    "felixkiss/uniquewith-validator": "2.*",
    "barryvdh/laravel-dompdf": "0.6.*"
  },
  "authors": [
    {
      "name": "Alejandro Granadillo",
      "email": "slayerfat@gmail.com"
    }
  ],
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "laravel/homestead": "^2.1",
    "barryvdh/laravel-debugbar": "^2.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "PCI\\": "app/",
      "PCI\\Database\\": "database/",
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php ../replace.php",
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "php ../replace.php",
      "php artisan optimize"
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
